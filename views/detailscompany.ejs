								<!-- Language Switcher -->
								<div class="footer-row">
									<div class="footer-row-inner">
										<label>Language : English</label>
									</div>
								</div>
							</div>

						</div>
						<!-- Footer Rows Container / End -->
					</div>
				</div>
			</div>
		</div>
		<!-- Footer Top Section / End -->

		<!-- Footer Copyrights -->
		<div class="footer-bottom-section">
			<div class="container">
				<div class="row">
					<div class="col-xl-12">
						© 2023 <strong>ShopZone</strong>. All Rights Reserved.
					</div>
				</div>
			</div>
		</div>
