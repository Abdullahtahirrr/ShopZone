												<option value="Yemen">Yemen</option>
												<option value="Zambia">Zambia</option>
												<option value="Zimbabwe">Zimbabwe</option>

											</select>
										</div>
									</div>

									<div class="col-xl-12">
										<div class="submit-field">
											<h5>Introduction to Company</h5>
											<textarea cols="30" rows="5" name="description" class="with-border"
												placeholder="A brief introduction of the company!" required></textarea>
										</div>
									</div>

								</div>
							</li>
							</ul>
						</div>
					</div>
				</div>



				<!-- Button -->
				<a class="btn btn-info" role="button"><button class="button ripple-effect margin-top-30" type="submit"
						form="EmployerDetails">Register <i
							class="icon-material-outline-arrow-right-alt"></i></button></a>


			</div>
			<!-- Row / End -->

		

		</div>
	</div>
	<!-- Dashboard Content / End -->

	</div>
	<!-- Dashboard Container / End -->
	<div id="footer">

		<!-- Footer Top Section -->
		<div class="footer-top-section">
			<div class="container">
				<div class="row">
					<div class="col-xl-12">

						<!-- Footer Rows Container -->
						<div class="footer-rows-container">

							<!-- Left Side -->
							<div class="footer-rows-left">
								<div class="footer-row">
									<div class="footer-row-inner footer-logo">
										<img src="/images/logo.png" alt="">
										<h1 style="font-size: 50px; color: blanchedalmond;">
											<strong>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;ShopZone </strong>
